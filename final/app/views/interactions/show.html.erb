<div class="page-header">
  <h3>Interaction #<%= @interaction.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Recruiter's Name</dt>
      <dd><%= @interaction.rec_id %></dd>

      <dt>Interaction Type</dt>
      <dd><%= @interaction.int_type %></dd>

      <dt>Interaction Score</dt>
      <dd><%= @interaction.int_score %></dd>

      <dt>School Affinity</dt>
      <dd><%= @interaction.school_affinity %></dd>

      <dt>Comments</dt>
      <dd><%= @interaction.user_comment %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/interactions" class="btn btn-primary">
        Back
      </a>
      <a href="/interactions/<%= @interaction.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_interaction/<%= @interaction.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

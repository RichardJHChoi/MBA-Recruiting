<div class="page-header">
  <h1>
    Interaction types
    <a href="/interaction_types/new" class="btn btn btn-success">New Interaction type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Int type</th>
        <th>Actions</th>
      </tr>

      <% @interaction_types.each do |interaction_type| %>
      <tr>
        <td><%= interaction_type.int_type %></td>
        <td>
          <a href="/interaction_types/<%= interaction_type.id %>" class="btn btn-primary">Show</a>
          <a href="/interaction_types/<%= interaction_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_interaction_type/<%= interaction_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

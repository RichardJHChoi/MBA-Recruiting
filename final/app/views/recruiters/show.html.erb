<div class="page-header">
  <h3>Recruiter #<%= @recruiter.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Rec name</dt>
      <dd><%= @recruiter.rec_name %></dd>

      <dt>Rec corp</dt>
      <dd><%= @recruiter.rec_corp %></dd>

      <dt>Rec school</dt>
      <dd><%= @recruiter.rec_school %></dd>

      <dt>Rec email</dt>
      <dd><%= @recruiter.rec_email %></dd>

      <dt>Rec mobile</dt>
      <dd><%= @recruiter.rec_mobile %></dd>

      <dt>Rec phone</dt>
      <dd><%= @recruiter.rec_phone %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/recruiters" class="btn btn-primary">
        Back
      </a>
      <a href="/recruiters/<%= @recruiter.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_recruiter/<%= @recruiter.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

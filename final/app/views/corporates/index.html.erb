<div class="page-header">
  <h1>
    Corporates
    <a href="/corporates/new" class="btn btn-lg btn-success">New Corporate</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Corp name</th>
        <th>Corp ind</th>
        <th>Corp reg</th>
        <th>Actions</th>
      </tr>

      <% @corporates.each do |corporate| %>
      <tr>
        <td><%= corporate.corp_name %></td>
        <td><%= corporate.corp_ind %></td>
        <td><%= corporate.corp_reg %></td>
        <td>
          <a href="/corporates/<%= corporate.id %>" class="btn btn-primary">Show</a>
          <a href="/corporates/<%= corporate.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_corporate/<%= corporate.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
